  "scripts": {
    "start:firebase": "firebase emulators:start --project test-tutorbook",
    "start:next": "cross-env NODE_OPTIONS='--inspect' next start -p 3000",
    "start:cy": "cypress run --record",
    "start": "concurrently yarn:start:firebase yarn:start:next",
    "dev:firebase": "firebase emulators:start --project development-tutorbook",
    "dev:next": "cross-env NODE_OPTIONS='--inspect' next dev -p 3000",
    "dev:cy": "cypress open",
    "dev": "concurrently yarn:dev:firebase yarn:dev:next yarn:dev:cy",
    "build:sw": "cd sw && webpack",
    "build:next": "next build",
    "build:analyze": "cross-env ANALYZE=true next build",
    "build:firebase": "cd firebase/functions && npm install && npm run build",
    "build": "concurrently yarn:build:sw yarn:build:next",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx .",
    "style": "pretty-quick --staged"
  },
